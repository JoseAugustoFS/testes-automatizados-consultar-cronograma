{
    "version": 2,
    "functions": {
      "api/index.ts": {
        "runtime": "vercel-node@canary"
      }
    },
    "builds": [
      {
        "src": "api/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/infra/routes/exemplo_router.ts"
      }
    ]
  }
  